<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainApp"
    tools:layout_editor_absoluteX="-1dp"
    tools:layout_editor_absoluteY="-74dp">

    <Button
        android:id="@+id/button4"
        android:layout_width="201dp"
        android:layout_height="46dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/btn_bg"
        android:backgroundTint="#03A9F4"
        android:text="Confirm"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="326dp"
        android:layout_height="46dp"
        android:layout_marginBottom="4dp"
        android:text="Date"
        android:textColor="#040404"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.305"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button9"
        android:layout_width="152dp"
        android:layout_height="43dp"
        android:layout_marginBottom="180dp"
        android:background="@drawable/btn_bg"
        android:backgroundTint="#03A9F4"
        android:text="Pick Date"
        android:textColor="#FFFFFF"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.413"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/spinnerMain"
        android:layout_width="378dp"
        android:layout_height="60dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="312dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/spinnerTwo"
        android:layout_width="369dp"
        android:layout_height="65dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="436dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="218dp"
        android:layout_height="178dp"
        android:layout_marginEnd="220dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/cube_box_light_removebg_preview" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="204dp"
        android:layout_height="118dp"
        android:text="Our Services"
        android:textColor="#2196F3"
        android:textSize="45dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.927"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.052" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="142dp"
        android:layout_height="41dp"
        android:text="location"
        android:textColor="#040404"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/spinnerMain"
        app:layout_constraintEnd_toEndOf="@+id/spinnerMain"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="@+id/spinnerMain" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="105dp"
        android:layout_height="44dp"
        android:text="Service"
        android:textColor="#040404"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/spinnerTwo"
        app:layout_constraintEnd_toEndOf="@+id/spinnerTwo"
        app:layout_constraintHorizontal_bias="0.056"
        app:layout_constraintStart_toStartOf="@+id/spinnerTwo" />

</androidx.constraintlayout.widget.ConstraintLayout>